	public GuiHandlerBonfire (){
		
	}
	
	@Override
	public Object getServerGuiElement(int ID, EntityPlayer player, World world, int x, int y, int z) {
		if(ID == 0){
			TileEntityBonfire tileEntityFurnace = (TileEntityBonfire) world.getTileEntity(x, y, z);
			return new TileEntityBonfire(player.inventory, TileEntityBonfire);
		}
		return null;
	}

